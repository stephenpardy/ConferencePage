body {
    width: 100%;
    height: 100%;
    padding-top: 350px;
}


html {
    width: 100%;
    height: 100%;
}

@media(min-width:767px) {
    .navbar {
        min-height: 50px;
        padding: 250px 0px;
        -webkit-transition: background .35s ease-in-out,padding .35s ease-in-out;
        -moz-transition: background .35s ease-in-out,padding .35s ease-in-out;
        transition: background .35s ease-in-out,padding .35s ease-in-out;
        background-image: url(/resources/MaltaParadisebeach.jpg);
        background-size: 100%;
        vertical-align: sub;
    }

    .top-nav-collapse {
        padding: 0;
        min-height: 50px;

    }
  body {
    padding-top: 350px;
  }
  .container {
    width: 100%;

  }
}

.nav.navbar-nav.navbar-nav li a {
    color: 51, 51, 51;
    font-size: 25px;
}
.navbar-default .navbar-brand {
    color: 51, 51, 51;
    font-size: 25px;
}

.collapse.navbar-collapse.navbar-ex1-collapse{
    background-color: rgba(255,255,255,0.5);
}
#navcontainer {
    position: absolute;
    bottom: 0;
    left: 0;
  }



